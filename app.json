{
  "expo": {
    "name": "Orda_Customer",
    "slug": "Orda_Customer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/gptOrdaLogo.png",
    "scheme": "ordacustomer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/gptOrdaLogo.png",
        "backgroundColor": "#70CFAF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.hephzibah108.Orda_Customer"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/gptOrdaLogo.png",
          "resizeMode": "contain",
          "backgroundColor": "#70CFAF",
          "enableFullScreen_legacy": true,
          "hideStatusBar_legacy": true,
          "hideHomeIndicator_legacy": true,
          "hideExponentText": true
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d357de0f-170a-4392-a09c-41ca52ddc214"
      }
    }
  },
  "experiments": {
    "typedRoutes": true
  },
  "extra": {
    "router": {},
    "eas": {
      "projectId": "fdb7da35-500e-4b7c-b668-374b18b1b2af"
    }
  }
}
